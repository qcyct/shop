.hotRanking .react-view {
  position: absolute;
  top: 0rpx;
  left: 6rpx;
  width: 50rpx;
  height: 62rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  z-index: 10;
}
.hotRanking .react-view .react-view-content {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 100%;
}
.hotRanking .react-view .react-view-content .react-view-img {
      position: absolute;
      top: 0rpx;
      left: 0rpx;
      width: 100%;
      height: 100%;
      background-image: url(data:image/png;base64,);
      background-size: cover;
      background-repeat: no-repeat;
      background-position: 50% center;
}
.hotRanking .react-view .react-view-content .react-view-img.react-view-img-1 {
        background-image: url(data:image/png;base64,);
}
