<view><block wx:if="{{showHeader}}"><view class="status" style="{{'position:'+(headerPosition)+';'+('top:'+(statusTop)+';')}}"></view></block><block wx:if="{{showHeader}}"><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><view class="addr"></view><view class="input-box"></view></view></block><block wx:if="{{showHeader}}"><view class="place"></view></block><block wx:if="{{isLoginShow}}"><view class="user"><view class="left"><image class="userImage" src="{{userinfo.userUrl}}" data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" bindtap="__e"></image></view><view class="right"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="username" bindtap="__e">{{userinfo.userName}}</view></view></view></block><block wx:else><view class="user"><view class="left"><view class="userImage" style="overflow:hidden;"><open-data type="userAvatarUrl"></open-data></view></view><view class="right"><view><button class="login-btn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">授权登录</button></view></view></view></block><view class="order"><tui-list-cell vue-id="1" arrow="{{true}}" padding="0" lineLeft="{{false}}" data-event-opts="{{[['^click',[['tomyOrder']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-cell-header"><view class="tui-cell-title">我的订单</view><view class="tui-cell-sub">查看全部订单</view></view></tui-list-cell><view class="list"><block wx:for="{{orderList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toOrderList',[index]]]]]}}" class="box" bindtap="__e"><view class="img"><view class="{{['icon',row.icon]}}"></view></view><view class="text">{{row.text}}</view></view></block></view></view><view class="toolbar"><view class="title">我的工具栏</view><view class="list"><block wx:for="{{mytoolbarList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['mytoolbarList','',index,'url']]]]]]]}}" class="box" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.text}}</view></view></block></view></view><view class="place-bottom"></view><fab vue-id="2" right="{{30}}" bottom="{{100}}" bgColor="#fff" width="{{80}}" height="{{80}}" bind:__l="__l" vue-slots="{{['default']}}"><button class="kfBtn" open-type="contact" session-from="weapp"><image src="../../static/img/kf.png" mode="widthFix"></image></button></fab></view>